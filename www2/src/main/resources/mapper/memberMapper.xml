<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="www.silver.dao.IF_MemberDao">
	<!-- parameterType은 쿼리문의 ?부분의 정보 -->
	<insert id="insertOne" parameterType="www.silver.vo.MemberVO">
		insert into kmember values (#{id},#{pass},#{tel},#{email})
	</insert>
	<insert id="insertWord" parameterType="www.silver.vo.wordVO">
		insert into wordlist values (#{eword},#{kword})
	</insert>
	<select id="selectAll" resultType="www.silver.vo.wordVO">
		select * from wordlist
	</select>

</mapper>

